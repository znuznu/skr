// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model Pokemon {
    id String @id @default(cuid())

    dex_id Int
    types String[]
    en_name String
    jp_name String?
    artwork_url String?
    sprite_url String?
}